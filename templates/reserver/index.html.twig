{% extends 'base.html.twig' %}
{% block title %}reserver{% endblock %}
{% block body %}


<div class="container-fluid">
    <!-- Page Heading -->
    <h1 class="h3 mb-4 text-gray-800">reserver </h1>
    <div class="row">
        <div class="col-lg-12">
            <table class="table" id="tableac">
                <thead>

                <tr>
                    <th>Id</th>
                    <th>iduser</th>
                    <th>idevent</th>
                    <th>nbrplace</th>



                    <!-- <th>Image</th> -->
                    <th>actions</th>
                </tr>
                </thead>
                <tbody>
                {% for event in events %}
                    <tr>
                        <td>{{ event.iduser }}</td>
                        <td>{{ event.idevent }}</td>

                        <td>{{ event.nbrplace }}</td>

                    </tr>


                {% endfor %}
                </tbody>
            </table>

            <a href="{{ path('reserver_new') }}">Create new</a>


        </div>
    </div>
</div>
{% endblock %}