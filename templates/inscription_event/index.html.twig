{% extends 'base.html.twig' %}

{% block title %}InscriptionEvent index{% endblock %}

{% block body %}
    <h1>InscriptionEvent index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DateCreation</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inscription_event in inscription_events %}
            <tr>
                <td>{{ inscription_event.id }}</td>
                <td>{{ inscription_event.dateCreation ? inscription_event.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('inscription_event_show', {'id': inscription_event.id}) }}">show</a>
                    <a href="{{ path('inscription_event_edit', {'id': inscription_event.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('inscription_event_new') }}">Create new</a>
{% endblock %}
